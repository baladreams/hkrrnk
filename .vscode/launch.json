    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "JavaScript: Current File",
            "env": {
                "NODE_PATH": "."
            },
            "program": "${file}",
            "runtimeExecutable": "/Users/bala/.nvm/versions/node/v16.5.0/bin/node"
        },
        {
            "type": "java",
            "request": "launch",
            "name": "Java: Current File",
            "projectName": "hkrrnk",
            "mainClass": "${file}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Go : Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "type": "scala",
            "request": "launch",
            "name": "Scala : Current File",
            "mainClass": "${fileName}",
            "args": [],
            "jvmOptions": [],
            "env": {}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TypeScript: Current File",
            "env": {
                "NODE_PATH": "."
            },
            "program": "${file}",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/out/**/*.js"],
            "runtimeExecutable": "/Users/bala/.nvm/versions/node/v16.5.0/bin/node"
        }
    ]
}